<!DOCTYPE html>
<html>
<head>
  <title>Nethips Patient Database</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <% if current_user %>
    <%= link_to "Home", root_path %> |
    <%= link_to "Upload Patients File", upload_patients_path %> |
    <%= link_to "Sign Out", destroy_user_session_path, :method => :delete %>
    <% if current_user.admin %>
      |
      <%= link_to "Users", users_path %> |
      <%= link_to "Archived Patients", archived_patients_path %>
    <% end %>
  <% end %>
  <br /> <br />
  <%= yield %>

</body>
</html>
